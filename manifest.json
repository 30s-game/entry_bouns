{
  "manifest_version": 3,
  "name": "엔트리 도우미",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://playentry.org/*"],
      "js": ["content.js"],
      "run_at": "document_end",
      "world": "MAIN"
    }
  ],
  "permissions": ["storage"]
}
